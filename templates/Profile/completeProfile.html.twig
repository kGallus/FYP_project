{{ include('base.html.twig') }}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Profile{% endblock %}</title>
        <link rel="icon" href="{{ absolute_url(asset('images/favicon.ico')) }}" />
        <style>

        </style>

        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        {% set bar = "" %}

        <div id="content" style="">

            <div id="user_form">
                {{ form(form)}}
            </div>

        </div>

        <script>

        </script>

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
